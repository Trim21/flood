{
  "name": "@flood/server",
  "version": "4.7.0",
  "description": "A modern Web UI for various torrent clients with multi-user and multi-client support",
  "keywords": [
    "typescript",
    "react",
    "node",
    "webui",
    "web ui",
    "rTorrent",
    "qBittorrent",
    "Transmission"
  ],
  "private": false,
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/jesec/flood.git"
  },
  "bugs": {
    "url": "https://github.com/jesec/flood/issues"
  },
  "files": [
    "dist",
    "node_modules",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "SECURITY.md"
  ],
  "bin": {
    "flood": "dist/index.js"
  },
  "pkg": {
    "assets": [
      "dist/assets/**/*",
      "dist/geoip-country.dat",
      "dist/geoip-country6.dat"
    ],
    "targets": [
      "node14-linuxstatic-x64",
      "node14-linuxstatic-arm64",
      "node14-macos-x64",
      "node14-macos-arm64",
      "node14-win-x64",
      "node14-win-arm64"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production webpack -c ./config/webpack.config.prod.js --mode production",
    "build-pkg": "rm -rf dist && npm run build && pkg . --out-path dist-pkg",
    "format-source": "prettier -w .",
    "check-source-formatting": "prettier -c .",
    "check-types": "tsc",
    "lint": "NODE_ENV=development eslint --max-warnings 0 . --ext .js --ext .jsx --ext .ts --ext .tsx",
    "prepack": "rm -rf dist && npm run build",
    "start": "node --use_strict dist/index.js",
    "dev": "NODE_ENV=development TS_NODE_PROJECT=tsconfig.json ts-node-dev --transpile-only -r tsconfig-paths/register bin/start.ts",
    "start:test:rtorrent": "node scripts/testsetup.js",
    "test": "jest --forceExit",
    "test:watch": "jest --watchAll --forceExit",
    "test:client": "FLOOD_OPTION_port=4200 start-server-and-test start 4200 'cypress run'"
  },
  "dependencies": {
    "geoip-country": "^4.1.58"
  },
  "devDependencies": {
    "@seald-io/nedb": "^3.1.0",
    "@types/bencode": "^2.0.1",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/content-disposition": "^0.5.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/create-torrent": "^5.0.0",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/geoip-country": "^4.0.0",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.185",
    "@types/morgan": "^1.9.3",
    "@types/node": "^12.20.55",
    "@types/parse-torrent": "^5.8.4",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "@types/tar-fs": "^2.0.1",
    "@types/yargs": "^17.0.12",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.21.2",
    "bencode": "^2.0.3",
    "body-parser": "^1.20.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "chalk": "^4.1.2",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "content-disposition": "^0.5.4",
    "cookie-parser": "^1.4.6",
    "create-torrent": "^5.0.6",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "express-rate-limit": "^6.6.0",
    "fast-json-patch": "^3.1.1",
    "fast-sort": "^3.2.0",
    "feedsub": "^0.7.8",
    "file-loader": "^6.2.0",
    "form-data": "^4.0.0",
    "fs-extra": "^10.1.0",
    "get-user-locale": "^1.5.1",
    "hash-wasm": "^4.9.0",
    "http-errors": "^2.0.0",
    "jest": "^28.1.3",
    "js-file-download": "^0.4.12",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "parse-torrent": "^9.1.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "sanitize-filename": "^1.6.3",
    "saxen": "^8.1.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "supertest": "^6.2.4",
    "tar-fs": "^2.1.1",
    "tldts": "^5.7.91",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typed-emitter": "^2.1.0",
    "typescript": "~4.8.3",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.11.0",
    "yargs": "^17.5.1",
    "zod": "^3.19.1"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "bundleDependencies": [
    "geoip-country"
  ]
}
