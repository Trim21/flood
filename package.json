{
  "name": "flood",
  "version": "4.7.0",
  "description": "A modern Web UI for various torrent clients with multi-user and multi-client support",
  "keywords": [
    "typescript",
    "react",
    "node",
    "webui",
    "web ui",
    "rTorrent",
    "qBittorrent",
    "Transmission"
  ],
  "private": false,
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/jesec/flood.git"
  },
  "bugs": {
    "url": "https://github.com/jesec/flood/issues"
  },
  "files": [
    "dist",
    "node_modules",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "SECURITY.md"
  ],
  "bin": {
    "flood": "dist/index.js"
  },
  "pkg": {
    "assets": [
      "dist/assets/**/*",
      "dist/geoip-country.dat",
      "dist/geoip-country6.dat"
    ],
    "targets": [
      "node14-linuxstatic-x64",
      "node14-linuxstatic-arm64",
      "node14-macos-x64",
      "node14-macos-arm64",
      "node14-win-x64",
      "node14-win-arm64"
    ]
  },
  "scripts": {
    "build": "pnpm run client build && pnpm run server build",
    "build-pkg": "rm -rf dist && pnpm run build && pkg . --out-path dist-pkg",
    "client": "pnpm --filter=@flood/client",
    "server": "pnpm --filter=@flood/server",
    "shared": "pnpm --filter=@flood/shared",
    "format-source": "prettier -w .",
    "check-source-formatting": "prettier -c .",
    "check-types": "tsc",
    "lint": "NODE_ENV=development eslint --max-warnings 0 . --ext .js --ext .jsx --ext .ts --ext .tsx",
    "prepack": "rm -rf dist && npm run build",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "start": "node --use_strict dist/index.js",
    "start:development:client": "node client/scripts/development.js",
    "start:development:server": "NODE_ENV=development TS_NODE_PROJECT=server/tsconfig.json ts-node-dev --transpile-only -r tsconfig-paths/register server/bin/start.ts",
    "start:test:rtorrent": "node scripts/testsetup.js",
    "test": "pnpm run server run test",
    "test:watch": "pnpm run server exec jest --watchAll --forceExit",
    "test:client": "FLOOD_OPTION_port=4200 start-server-and-test start 4200 'cypress run'"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,jsx,ts,tsx,yaml,yml}": "prettier -w"
  },
  "dependencies": {
    "geoip-country": "^4.1.21"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@types/node": "^12.20.55",
    "@types/yargs": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "debug": "^4.3.4",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-module-utils": "^2.8.0",
    "eslint-plugin-import": ">=2.25.3 <3.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "get-user-locale": "^1.5.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.7.1",
    "saxen": "^8.1.2",
    "supertest": "^6.2.4",
    "typescript": "~4.8.3",
    "yargs": "^17.5.1",
    "zod": "^3.19.1"
  },
  "packageManager": "pnpm@8.5.1",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "bundleDependencies": [
    "geoip-country"
  ]
}
